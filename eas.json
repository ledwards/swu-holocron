{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "credentialsSource": "remote"
      }
    },
    "testflight": {
      "extends": "production",
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "buildConfiguration": "Release",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    },
    "testflight": {
      "ios": {}
    }
  }
}

